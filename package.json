{
  "name": "rippingyard.com",
  "version": "1.0.0",
  "description": "lifelog for living well",
  "author": "mcatm",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "fb": "cross-env NODE_ENV=development firebase serve",
    "build:nuxt": "nuxt build",
    "build:functions": "cd ./functions && npm run build",
    "build": "nuxt build && rm -rf dist && mkdir -p dist && cp -R functions dist/server && cp -R .nuxt dist/server/ && cp -R static dist/client && mkdir -p dist/client/_nuxt && cp -R .nuxt/dist/client/* dist/client/_nuxt",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "clear": "rm -rf ./nuxt"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/google-adsense": "^1.1.3",
    "@nuxtjs/gtm": "^2.2.3",
    "@nuxtjs/sentry": "^4.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "en-inflectors": "^1.0.12",
    "express": "^4.16.4",
    "firebase": "^7.8.2",
    "minimist": ">=0.2.1",
    "moment": "^2.25.3",
    "node-sass": "^4.13.1",
    "nuxt": "^2.12.2",
    "nuxt-buefy": "^0.3.28",
    "nuxt-svg-loader": "^1.2.0",
    "nuxt-vuex-localstorage": "^1.2.6",
    "sass-loader": "^8.0.2",
    "tiptap": "^1.26.6",
    "tiptap-extensions": "^1.28.6",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "vue-jest": "^3.0.3"
  },
  "resolutions": {
    "prosemirror-model": "1.9.1"
  }
}
