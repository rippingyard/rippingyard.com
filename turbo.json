{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:www": {
      "cache": false,
      "persistent": true
    },
    "build:sb": {
      "cache": false,
      "persistent": false
    },
    "sb": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    }
  }
}
